{
    "AMAZON_LTL":{
        "SVR": [
            "log_weight",
            "log_Hdis",
            "across_state",
            "from_state_encoded_AMAZON_LTL",
            "vendor_name_encoded_AMAZON_LTL"
        ],
        "CatBoost": [
            "across_state",
            "from_state",
            "from_city",
            "log_Hdis",
            "log_weight"
